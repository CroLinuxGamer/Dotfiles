#!/bin/sh

nbr=$1

case $nbr in
    1) bspc desktop -f $BSPWM_D1 ;;
    2) bspc desktop -f $BSPWM_D2 ;;
    3) bspc desktop -f $BSPWM_D3 ;;
    4) bspc desktop -f $BSPWM_D4 ;;
    5) bspc desktop -f $BSPWM_D5 ;;
    6) bspc desktop -f $BSPWM_D6 ;;
    7) bspc desktop -f $BSPWM_D7 ;;
    8) bspc desktop -f $BSPWM_D8 ;;
    9) bspc desktop -f $BSPWM_D9 ;;
esac
