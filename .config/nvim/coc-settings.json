{
    // Diagnostics settings
    "diagnostic.enable": true,
    "diagnostic.level": "hint",
    "diagnostic.messageTarget": "float",
    "diagnostic.enableMessage": "jump",
    "diagnostic.filetypeMap": {},
    "diagnostic.infoSign": ">>",
    "diagnostic.hintSign": ">>",
    "diagnostic.errorSign": ">>",
    "diagnostic.enableSign": true,
    "diagnostic.signOffset": 1000,
    "diagnostic.warningSign": "⚠ ",
    "diagnostic.messageDelay": 200,
    "diagnostic.virtualText": true,
    "diagnostic.maxWindowHeight": 4,
    "diagnostic.maxWindowWidth": 40,
    "diagnostic.displayByAle": false,
    "diagnostic.virtualTextLines": 3,
    "diagnostic.highlightOffset": 1000,
    "diagnostic.virtualTextPrefix": " ",
    "diagnostic.checkCurrentLine": true,
    "diagnostic.refreshAfterSave": true,
    "diagnostic.refreshOnInsertMode": true,
    "diagnostic.enableHighlightLineNumber": true,
    "diagnostic.virtualTextCurrentLineOnly": true,
    "diagnostic.virtualTextLineSeparator": " \\ ",
    "diagnostic.format": "%message\n[%source:%code]",
    "diagnostic.separateRelatedInformationAsDiagnostics": true,

    // Json
    "json.enable": true,
    "json.schemas": [],
    "json.execArgv": [],
    "json.trace.server": "verbose",
    "json.format.enable": true,
    
    // signature
    "signature.target": "float",
    "signature.enable": true,
    "signature.floatMaxWidth": 30,
    "signature.maxWindowHeight": 4,
    "signature.preferShownAbove": true,
    "signature.hideOnTextChange": true,
    "signature.triggerSignatureWait": 50,

    // list
    "list.minHeight": 1,
    "list.maxHeight": 10,
    "list.indicator": ">",
    "list.signOffset": 900,
    "list.normalMappings": {},
    "list.insertMappings": {},
    "list.autoResize": true,
    "list.limitLines": 15000,
    "list.nextKeymap": "<C-j>",
    "list.maxPreviewHeight": 6,
    "list.selectedSignText": "*",
    "list.previousKeymap": "<C-k>",
    "list.extendedSearchMode": true,
    "list.previewSplitRight": false,
    "list.source.symbols.excludes": [],
    "list.source.links.defaultArgs": [],
    "list.source.lists.defaultArgs": [],
    "list.source.tasks.defaultArgs": [],
    "list.source.output.defaultArgs": [],
    "list.source.symbols.defaultOptions": [
        "--interactive"
    ],
    "list.source.symbols.defaultArgs": [],
    "list.source.outline.defaultArgs": [],
    "list.source.sources.defaultArgs": [],
    "list.source.folders.defaultArgs": [],
    "list.source.actions.defaultArgs": [],
    "list.interactiveDebounceTime": 100,
    "list.source.links.defaultOptions": [],
    "list.source.location.defaultArgs": [],
    "list.source.commands.defaultArgs": [],
    "list.source.services.defaultArgs": [],
    "list.source.lists.defaultOptions": [],
    "list.source.tasks.defaultOptions": [],
    "list.source.output.defaultOptions": [],
    "list.source.outline.defaultOptions": [],
    "list.source.extensions.defaultArgs": [],
    "list.source.sources.defaultOptions": [],
    "list.source.folders.defaultOptions": [],
    "list.source.actions.defaultOptions": [],
    "list.previewHighlightGroup": "Search",
    "list.source.outline.ctagsFilestypes": [],
    "list.source.location.defaultOptions": [],
    "list.source.commands.defaultOptions": [],
    "list.source.diagnostics.defaultArgs": [],
    "list.source.services.defaultOptions": [],
    "list.source.marketplace.defaultArgs": [],
    "list.source.extensions.defaultOptions": [],
    "list.source.diagnostics.defaultOptions": [],
    "list.source.marketplace.defaultOptions": [],

    // tabnine
    "tabnine.triggers": [],
    "tabnine.limit": 10,
    "tabnine.shortcut": "TN",
    "tabnine.priority": null,
    "tabnine.binary_path": "",
    "tabnine.disable_filetypes": [],
    "tabnine.disable_file_regex": [],
    "tabnine.disable_line_regex": [],

    // http
    "http.proxy": "",
    "http.proxyStrictSSL": true,

    // clangd
    "clangd.arguments": ["-header-insertion=never", "--suggest-missing-includes"],
    "clangd.enabled": true,
    "clangd.path": "clangd",
    "clangd.fallbackFlags": [],
    "clangd.checkUpdates": false,
    "clangd.disableDiagnostics": false,
    "clangd.semanticHighlighting": true,
    "clangd.disableSnippetCompletion": false,
    "clangd.showDBChangedNotification": true,
    
    // suggest
    "suggest.detailField": "preview",
    "suggest.autoTrigger": "always",
    "suggest.timeout": 2500,
    "suggest.noselect": true,
    "suggest.defaultSortMethod": "length",
    "suggest.floatEnable": true,
    "suggest.disableKind": false,
    "suggest.disableMenu": false,
    "suggest.maxPreviewWidth": 40,
    "suggest.labelMaxLength": 100,
    "suggest.detailMaxLength": 25,
    "suggest.numberSelect": false,
    "suggest.localityBonus": true,
    "suggest.enablePreview": true,
    "suggest.snippetIndicator": "~",
    "suggest.fixInsertedWord": true,
    "suggest.echodocSupport": false,
    "suggest.invalidInsertCharacters": [
        " ",
        "(",
        "<",
        "{",
        "[",
        "\r",
        "\n"
    ],
    "suggest.enablePreselect": false,
    "suggest.keepCompleteopt": false,
    "suggest.maxCompleteItemCount": 50,
    "suggest.minTriggerInputLength": 1,
    "suggest.triggerCompletionWait": 50,
    "suggest.lowPrioritySourceLimit": 10,
    "suggest.completionItemKindLabels": {},
    "suggest.languageSourcePriority": 99,
    "suggest.disableMenuShortcut": false,
    "suggest.highPrioritySourceLimit": 10,
    "suggest.asciiCharactersOnly": false,
    "suggest.removeDuplicateItems": false,
    "suggest.triggerAfterInsertEnter": true,
    "suggest.preferCompleteThanJumpPlaceholder": false,
    "suggest.acceptSuggestionOnCommitCharacter": false,

    // codeLens
    "codeLens.enable": false,
    "codeLens.separator": "‣",
    "codeLens.subseparator": " ",
   
    // refactor
    "refactor.afterContext": 3,
    "refactor.beforeContext": 3,
    "refactor.openCommand": "vsplit",

    // cursors
    "cursors.nextKey": "<C-n>",
    "cursors.cancelKey": "<esc>",
    "cursors.previousKey": "<C-p>",
    
    // coc
    "coc.preferences.rootPatterns": [
        ".git",
        ".hg",
        ".projections.json",
        ".root"
    ],
    "coc.source.file.trimSameExts": [
        ".ts",
        ".js"
    ],
    "coc.preferences.hoverTarget": "float",
    "coc.preferences.messageLevel": "error",
    "coc.source.word.priority": 0,
    "coc.source.file.enable": true,
    "coc.source.file.priority": 10,
    "coc.source.word.enable": true,
    "coc.source.around.priority": 1,
    "coc.source.buffer.priority": 1,
    "coc.source.file.shortcut": "F",
    "coc.source.around.enable": true,
    "coc.source.buffer.enable": true,
    "coc.source.file.triggerCharacters": [
        "/"
    ],
    "coc.source.around.shortcut": "A",
    "coc.source.buffer.shortcut": "B",
    "coc.source.file.ignorePatterns": [],
    "coc.preferences.colorSupport": true,
    "coc.source.word.shortcut": "10K",
    "coc.source.word.filetypes": null,
    "coc.source.file.disableSyntaxes": [],
    "coc.source.word.disableSyntaxes": [],
    "coc.preferences.listOfWorkspaceEdit": "quickfix",
    "coc.preferences.watchmanPath": null,
    "coc.source.around.firstMatch": true,
    "coc.source.around.disableSyntaxes": [],
    "coc.source.buffer.firstMatch": true,
    "coc.source.buffer.disableSyntaxes": [],
    "coc.source.file.ignoreHidden": true,
    "coc.preferences.maxFileSize": "25MB",
    "coc.preferences.extensionUpdateCheck": "never",
    "coc.preferences.jumpCommand": "edit",
    "coc.preferences.formatOnType": false,
    "coc.preferences.currentFunctionSymbolAutoUpdate": false,
    "coc.preferences.previewMaxHeight": 6,
    "coc.preferences.highlightTimeout": 500,
    "coc.preferences.snippets.enable": true,
    "coc.preferences.previewAutoClose": true,
    "coc.preferences.formatOnSaveFiletypes": [],
    "coc.preferences.formatOnTypeFiletypes": [],
    "coc.source.buffer.ignoreGitignore": true,
    "coc.preferences.promptWorkspaceEdit": true,
    "coc.preferences.snippetStatusText": "SNIP",
    "coc.preferences.bracketEnterImprove": true,
    "coc.preferences.formatOnInsertLeave": false,
    "coc.preferences.enableFloatHighlight": true,
    "coc.preferences.useQuickfixForLocations": false,

    // misc
    "marketplace.npmsio": false,
    "languageserver": {},
    "npm.binPath": "npm",

    // html
    "html.filetypes": [
        "html",
        "handlebars",
        "htmldjango",
        "blade"
    ],
    "html.execArgv": [],
    "html.trace.server": "off",
    "html.enable": true,
    "html.format.enable": true,
    "html.suggest.ionic": true,
    "html.suggest.html5": true,
    "html.validate.html": true,
    "html.format.wrapAttributes": "auto",
    "html.validate.styles": true,
    "html.suggest.angular1": true,
    "html.validate.scripts": true,
    "html.autoClosingTags": true,
    "html.experimental.custom.tags": [],
    "html.format.unformatted": "wbr",
    "html.format.wrapLineLength": 120,
    "html.format.endWithNewline": false,
    "html.format.indentInnerHtml": false,
    "html.format.preserveNewLines": true,
    "html.format.indentHandlebars": false,
    "html.experimental.custom.attributes": [],
    "html.format.maxPreserveNewLines": null,
    "html.format.extraLiners": "head, body, /html",
    "html.format.contentUnformatted": "pre,code,textarea",

    // git
    "git.gitlab.hosts": [
        "gitlab.com"
    ],
    "git.command": "git",
    "git.signOffset": 99,
    "git.diffRevision": "",
    "git.issueFormat": "#%i",
    "git.enableGutters": true,
    "git.pushArguments": null,
    "git.addedSign.text": "+",
    "git.remoteName": "origin",
    "git.branchCharacter": "",
    "git.stagedDecorator": "●",
    "git.changedDecorator": "*",
    "git.realtimeGutters": true,
    "git.changedSign.text": "~",
    "git.removedSign.text": "_",
    "git.semanticCommit.filetypes": [
        "gitcommit",
        "gina-commit"
    ],
    "git.blameUseRealTime": false,
    "git.untrackedDecorator": "…",
    "git.conflictedDecorator": "x",
    "git.enableGlobalStatus": true,
    "git.topRemovedSign.text": "‾",
    "git.virtualTextPrefix": "     ",
    "git.semanticCommit.scope": true,
    "git.addGBlameToBufferVar": false,
    "git.changeRemovedSign.text": "≃",
    "git.showCommitInFloating": false,
    "git.addedSign.hlGroup": "DiffAdd",
    "git.addGBlameToVirtualText": false,
    "git.splitWindowCommand": "above sp",
    "git.changedSign.hlGroup": "DiffChange",
    "git.removedSign.hlGroup": "DiffDelete",
    "git.topRemovedSign.hlGroup": "DiffDelete",
    "git.changeRemovedSign.hlGroup": "DiffChange"

}
