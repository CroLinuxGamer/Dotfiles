{
    // diagnostic
    "diagnostic.enable": true,
    "diagnostic.level": "hint",
    "diagnostic.messageTarget":"echo",
    "diagnostic.enableMessage":"jump",
    "diagnostic.infoSign": ">>",
    "diagnostic.hintSign": "??",
    "diagnostic.errorSign": "!!",
    "diagnostic.enableSign": true,
    "diagnostic.signPriority": 10,
    "diagnostic.showUnused": true,
    "diagnostic.messageDelay": 200,
    "diagnostic.virtualText":false,
    "diagnostic.warningSign": "⚠",
    "diagnostic.virtualTextLines": 3,
    "diagnostic.showDeprecated": true,
    "diagnostic.refreshOnInsertMode": true,
    "diagnostic.enableHighlightLineNumber": true,
    "diagnostic.format": "%message\n[%source:%code]",

    //  suggest
    "suggest.detailField": "preview",
    "suggest.autoTrigger": "none",
    "suggest.timeout": 5000,
    "suggest.noselect": true,
    "suggest.defaultSortMethod":"length",
    "suggest.floatEnable": true,
    "suggest.enablePreview": true,
    "suggest.fixInsertedWord": true,
    "suggest.languageSourcePriority": 99,
    "suggest.removeDuplicateItems": true,

    // Json
    "json.enable": true,
    "json.schemas": [],
    "json.execArgv": [],
    "json.trace.server": "verbose",
    "json.format.enable": true,
    
    // tabnine
    "tabnine.triggers": [],
    "tabnine.limit": 10,
    "tabnine.shortcut": "TN",
    "tabnine.priority": null,
    "tabnine.binary_path": "",
    "tabnine.disable_filetypes": [],
    "tabnine.disable_file_regex": [],
    "tabnine.disable_line_regex": [],

    // http
    "http.proxy": "",
    "http.proxyStrictSSL": true,

    // clangd
    "clangd.arguments": ["-header-insertion=never", "--suggest-missing-includes"],
    "clangd.enabled": true,
    "clangd.path": "clangd",
    "clangd.fallbackFlags": [],
    "clangd.checkUpdates": false,
    "clangd.disableDiagnostics": false,
    "clangd.semanticHighlighting": true,
    "clangd.disableSnippetCompletion": false,
    "clangd.showDBChangedNotification": true,
    
    // html
    "html.filetypes": [
        "html",
        "handlebars",
        "htmldjango",
        "blade"
    ],
    "html.execArgv": [],
    "html.trace.server": "off",
    "html.enable": true,
    "html.format.enable": true,
    "html.suggest.ionic": true,
    "html.suggest.html5": true,
    "html.validate.html": true,
    "html.format.wrapAttributes": "auto",
    "html.validate.styles": true,
    "html.suggest.angular1": true,
    "html.validate.scripts": true,
    "html.autoClosingTags": true,
    "html.experimental.custom.tags": [],
    "html.format.unformatted": "wbr",
    "html.format.wrapLineLength": 120,
    "html.format.endWithNewline": false,
    "html.format.indentInnerHtml": false,
    "html.format.preserveNewLines": true,
    "html.format.indentHandlebars": false,
    "html.experimental.custom.attributes": [],
    "html.format.maxPreserveNewLines": null,
    "html.format.extraLiners": "head, body, /html",
    "html.format.contentUnformatted": "pre,code,textarea",

    // git
    "git.gitlab.hosts": [
        "gitlab.com"
    ],
    "git.command": "git",
    "git.signOffset": 99,
    "git.diffRevision": "",
    "git.issueFormat": "#%i",
    "git.enableGutters": true,
    "git.pushArguments": null,
    "git.addedSign.text": "+",
    "git.remoteName": "origin",
    "git.branchCharacter": "",
    "git.stagedDecorator": "●",
    "git.changedDecorator": "*",
    "git.realtimeGutters": true,
    "git.changedSign.text": "~",
    "git.removedSign.text": "_",
    "git.semanticCommit.filetypes": [
        "gitcommit",
        "gina-commit"
    ],
    "git.blameUseRealTime": false,
    "git.untrackedDecorator": "…",
    "git.conflictedDecorator": "x",
    "git.enableGlobalStatus": true,
    "git.topRemovedSign.text": "‾",
    "git.virtualTextPrefix": "     ",
    "git.semanticCommit.scope": true,
    "git.addGBlameToBufferVar": false,
    "git.changeRemovedSign.text": "≃",
    "git.showCommitInFloating": false,
    "git.addedSign.hlGroup": "DiffAdd",
    "git.addGBlameToVirtualText": false,
    "git.splitWindowCommand": "above sp",
    "git.changedSign.hlGroup": "DiffChange",
    "git.removedSign.hlGroup": "DiffDelete",
    "git.topRemovedSign.hlGroup": "DiffDelete",
    "git.changeRemovedSign.hlGroup": "DiffChange",
    
    // omnisharp
    "omnisharp.path": "",
    "omnisharp.trace.server": "verbose",
    "omnisharp.version": "latest",
    "omnisharp.debug.server": true,

    // sh
    "sh.trace.server": "off",
    "sh.enable": true,
    "sh.commandPath": "",
    "sh.globPattern": "",
    "sh.explainshellEndpoint": "",
    "sh.highlightParsingErrors": false,

    // python
    "jedi.trace.server": "off",
    "jedi.enable": true,
    "jedi.executable.args": [],
    "jedi.markupKindPreferred":"markdown",
    "jedi.startupMessage": false,
    "jedi.diagnostics.enable":true,
    "jedi.diagnostics.didOpen":true,
    "jedi.diagnostics.didSave": true,
    "jedi.executable.command": "jedi-language-server",
    "jedi.diagnostics.didChange":true,
    "jedi.completion.disableSnippets": true
}
